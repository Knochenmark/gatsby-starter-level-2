{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "defaultSeverity": "warning",
  "rules": {
    "no-console": false,
    "no-unused-variable": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
